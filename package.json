{
  "name": "fetch-mcp",
  "version": "1.0.0",
  "description": "MCP server that provides a web_fetch tool as a transparent HTTP proxy",
  "type": "module",
  "bin": {
    "fetch-mcp": "./build/index.js"
  },
  "scripts": {
    "build": "tsc && chmod 755 build/index.js",
    "start": "node build/index.js",
    "dev": "npm run build && npm start",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "mcp",
    "fetch",
    "http",
    "proxy"
  ],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "express": "^4.18.2",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/nock": "^10.0.3",
    "@types/node": "^20.0.0",
    "@types/supertest": "^6.0.3",
    "@vitest/ui": "^4.0.6",
    "nock": "^14.0.10",
    "supertest": "^7.1.4",
    "typescript": "^5.3.0",
    "vitest": "^4.0.6"
  },
  "files": [
    "build"
  ]
}
